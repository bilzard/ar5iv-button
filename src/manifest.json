{
    "manifest_version": 3,
    "name": "Ar5iv Button",
    "version": "1.0",
    "description": "Insert ar5iv link button into arXiv.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "css": [
                "styles/content.css"
            ],
            "matches": [
                "https://arxiv.org/*"
            ]
        }
    ]
}
